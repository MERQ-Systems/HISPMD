			{"text":"{$jspagetitlelabel DHIS2_Analytics_Report rprint}","hidden":"{$item_hide_print_header}","fontSize":30},
				
				
			{"text":"{$printtabheader_text}","hidden":"{$item_hide_print_subheader}"},
				
		  ],
  "border": [ false, false, false, false ],
